<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="amr">

    <!--Gazebo Wheel Macro-->
    <xacro:macro name="gazebo_amr_wheel" params="
      wheel_link
      ">
        <gazebo reference="${wheel_link}">
            <mu1>0.05</mu1>
            <mu2>0.05</mu2>
            <kp>100000.0</kp>
            <kd>10.0</kd>
        </gazebo>
    </xacro:macro>

</robot>